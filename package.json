    "@sanity/ui": "^0.37.7"
  },
  "peerDependencies": {
    "@sanity/base": "^2.36.2",
    "@sanity/desk-tool": "^2.36.2",
    "@sanity/react-hooks": "^2.36.2",
    "country-flag-icons": "^1.5.10",
    "groq": "^3.36.2",
    "react": "^18.2.0",
    "react-query": "^3.39.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "sanipack": "^2.1.0"
  }
}

{
  "name": "@kaliber/sanity-plugin-multi-language",
  "version": "2.2.0-beta1",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "multi-language"
  ],
  "license": "MIT",
  "main": "lib/multi-language.js",
  "files": [
    "src/**",
    "lib/**",
    "sanity.json",
